ACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AAAA,2EAAA,iBAAA,KAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,KADA;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAEA,kBAAA,MAAA,CAAA,MAAA,CACA,IADA,CACA,WADA,EACA;AACA,oBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA,QADA;AAEA,oBAAA,QAAA,EAAA,MAAA,CAAA,QAAA,CAAA,QAFA;AAGA;AACA,oBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAJA,mBADA,EAOA,IAPA,CAOA,UAAA,IAAA,EAAA;AACA,oBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,wBAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,sBAAA,MAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA,EAAA,IAAA,EAAA;AACA,wBAAA,iBAAA,EAAA;AADA,uBAAA;;AAGA,sBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,qBALA,MAKA;AACA,sBAAA,MAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,IAAA,EAAA;AACA,wBAAA,iBAAA,EAAA;AADA,uBAAA;AAGA;AACA,mBAnBA,EAoBA,KApBA,CAoBA,UAAA,YAAA,EAAA,CACA,CArBA,EAFA,CAwBA;;;AAxBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AA0BA,KA/BA;AAgCA,IAAA,SAhCA,uBAgCA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,KAAA;AACA,WAAA,MAAA,CACA,IADA,CACA,sBAAA,KAAA,QAAA,CAAA,KADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,KAAA,CAAA,cAAA,EAAA,IAAA,EAAA;AACA,YAAA,iBAAA,EAAA;AADA,WAAA;;AAGA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,IAAA,EAAA;AACA,YAAA,iBAAA,EAAA;AADA,WAAA;AAGA;AACA,OAbA;AAcA,KAjDA;AAkDA,IAAA,MAlDA,oBAkDA;AAAA;;AACA,UAAA,KAAA,GAAA,IAAA;;AACA,WAAA,MAAA,CACA,IADA,CACA,wBAAA,KAAA,QAAA,CAAA,YADA,EAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,YAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,IAAA,EAAA;AACA,YAAA,iBAAA,EAAA;AADA,WAAA;AAGA;AACA,OAZA;AAaA;AAjEA;AA9EA,CAAA","sourcesContent":["<template>\r\n  <div class=\"sign_container\">\r\n    <div class=\"sign_box\">\r\n      <!-- 头像区域 -->\r\n      <div class=\"avatar_box\">\r\n        <img src=\"../assets/logo.png\" alt=\"\">\r\n      </div>\r\n      <!-- 登录表单区域 -->\r\n      <el-form ref=\"signFormRef\" :model=\"signForm\" :rules=\"signFormRules\" label-width=\"80px\" class=\"sign_form\" :label-position=\"labelPosition\">\r\n        <!-- 用户名 -->\r\n        <el-form-item prop=\"username\" label=\"用户名\">\r\n          <el-input v-model=\"signForm.username\" ></el-input>\r\n        </el-form-item>\r\n        <!-- 密码 -->\r\n        <el-form-item prop=\"password\" label=\"密码\">\r\n          <el-input v-model=\"signForm.password\" type=\"password\"></el-input>\r\n        </el-form-item>\r\n        <!-- 确认密码 -->\r\n        <el-form-item prop=\"repassword\" label=\"确认密码\">\r\n          <el-input v-model=\"signForm.repassword\" type=\"password\"></el-input>\r\n        </el-form-item>\r\n        <!-- 用户邮箱 -->\r\n        <el-form-item prop=\"email\" label=\"邮箱\">\r\n          <el-input v-model=\"signForm.email\" ></el-input>\r\n        </el-form-item>\r\n        <!-- 验证码 -->\r\n        <el-form-item prop=\"verification\" label=\"验证码\">\r\n          <el-input v-model=\"signForm.verification\"></el-input>\r\n        </el-form-item>\r\n        <!-- 按钮区域 -->\r\n        <el-form-item class=\"btns\">\r\n          <el-button type=\"info\" @click=\"sendEmail\">获取验证码</el-button>\r\n          <el-button type=\"primary\" @click=\"verify\">注册</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    const validatePass = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请输入密码'))\r\n      } else {\r\n        if (this.signForm.repassword !== '') {\r\n          this.$refs.signForm.validateField('repassword')\r\n        }\r\n        callback()\r\n      }\r\n    }\r\n    const validatePass2 = (rule, value, callback) => {\r\n      if (value === '') {\r\n        callback(new Error('请再次输入密码'))\r\n      } else if (value !== this.signForm.password) {\r\n        callback(new Error('两次输入密码不一致!'))\r\n      } else {\r\n        callback()\r\n      }\r\n    }\r\n    const validateEmail = (rule, value,callback) => {\r\n      const reg =/^([a-zA-Z0-9]+[-_\\.]?)+@[a-zA-Z0-9]+\\.[a-z]+$/;\r\n      if(value===''||value===undefined||value===null){\r\n        callback();\r\n      }else{\r\n        if (!reg.test(value)){\r\n          callback(new Error('请输入正确的邮箱'));\r\n        } else {\r\n          callback();\r\n        }\r\n      }\r\n    }\r\n    return {\r\n      labelPosition: 'right',\r\n      // 这是登录表单的数据绑定对象\r\n      signForm: {\r\n        userid: '',\r\n        username: '',\r\n        password: '',\r\n        repassword: '',\r\n        email: '',\r\n        verification: ''\r\n      },\r\n      // 这是表单的验证规则对象\r\n      signFormRules: {\r\n        // 验证用户名是否合法\r\n        // userid: [\r\n        //   { required: true, message: '请输入登录账号', trigger: 'blur' },\r\n        //   { min: 7, max: 7, message: '长度为 7 个字符', trigger: 'blur' }\r\n        // ],\r\n        username: [\r\n          { required: true, message: '请输入登录名称', trigger: 'blur' },\r\n          { min: 3, max: 10, message: '长度在 3 到 10 个字符', trigger: 'blur' }\r\n        ],\r\n        // 验证密码是否合法\r\n        password: [\r\n          { required: true, message: '请输入登录密码', trigger: 'blur' },\r\n          { min: 6, max: 15, message: '长度在 6 到 15 个字符', trigger: 'blur' },\r\n          { validator: validatePass, trigger: 'blur' }\r\n        ],\r\n        repassword: [\r\n          { required: true, message: '请确认登录密码', trigger: 'blur' },\r\n          { min: 6, max: 15, message: '长度在 6 到 15 个字符', trigger: 'blur' },\r\n          { validator: validatePass2, trigger: 'blur' }\r\n        ],\r\n        email: [\r\n          { required: true, message: '请输入注册邮箱', trigger: 'blur' },\r\n          { validator: validateEmail, trigger: 'blur' }\r\n          // { min: 6, max: 15, message: '长度在 6 到 15 个字符', trigger: 'blur' }\r\n        ],\r\n        verification: [\r\n          { required: true, message: '请输入验证码', trigger: 'blur' },\r\n          { min: 8, max: 8, message: '长度为 8 个字符', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    // 点击注册按钮\r\n    sign () {\r\n      console.log(\"注册方法调用啦\")\r\n      let _this = this\r\n      this.$refs.signFormRef.validate(async valid => {\r\n        if (!valid) return\r\n        this.$axios\r\n          .post('/register', {\r\n            username: this.signForm.username,\r\n            password: this.signForm.password,\r\n            // name: this.signForm.name,\r\n            email: this.signForm.email\r\n          })\r\n          .then(resp => {\r\n            console.log(resp)\r\n            if (resp.data.code === 200) {\r\n              this.$mb.alert('注册成功', '提示', {\r\n                confirmButtonText: '确定'\r\n              })\r\n              _this.$router.push('/login')\r\n            } else {\r\n              this.$mb.alert(resp.data.msg, '提示', {\r\n                confirmButtonText: '确定'\r\n              })\r\n            }\r\n          })\r\n          .catch(failResponse => {\r\n          })\r\n        // this.$router.push('/login')\r\n      })\r\n    },\r\n    sendEmail(){\r\n      let _this = this\r\n      console.log(this.signForm.email)\r\n      this.$axios\r\n        .post('/sendEmail?email='+this.signForm.email)\r\n        .then(resp =>{\r\n          if (resp.data.code === 200) {\r\n            this.$mb.alert('邮件已发送，请您及时查收', '提示', {\r\n              confirmButtonText: '确定'\r\n            })\r\n            _this.$router.push('/login')\r\n          } else {\r\n            this.$mb.alert(resp.data.msg, '提示', {\r\n              confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n    },\r\n    verify(){\r\n      let _this = this\r\n      this.$axios\r\n        .post('/verify?emailToken='+ this.signForm.verification)\r\n        .then(resp =>{\r\n          console.log('verify:'+resp)\r\n          if (resp.data.code === 200) {\r\n            // 若验证码正确，发送注册信息\r\n            _this.sign()\r\n          } else {\r\n            this.$mb.alert(resp.data.msg, '提示', {\r\n              confirmButtonText: '确定'\r\n            })\r\n          }\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.sign_container {\r\n  //background-color: #2b4b6b;\r\n  //height: 100%;\r\n  background: url(\"../assets/img/JiadingCampus.png\") no-repeat center;\r\n  height: 100%;\r\n  width: 100%;\r\n  background-size: cover;\r\n  position: fixed;\r\n}\r\n\r\n.sign_box {\r\n  width: 630px;\r\n  height: 420px;\r\n  background-color: #fff;\r\n  border-radius: 3px;\r\n  position: absolute;\r\n  opacity: 0.85;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n  .avatar_box {\r\n    height: 100px;\r\n    width: 100px;\r\n    border: 1px solid #eee;\r\n    border-radius: 50%;\r\n    padding: 10px;\r\n    box-shadow: 0 0 10px #ddd;\r\n    position: absolute;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    background-color: #fff;\r\n    img {\r\n      width: 100%;\r\n      height: 100%;\r\n      border-radius: 50%;\r\n      background-color: #eee;\r\n    }\r\n  }\r\n}\r\n\r\n.sign_form {\r\n  position: absolute;\r\n  bottom: 0;\r\n  top: 10%;\r\n  width: 90%;\r\n  padding-left: 10%;\r\n  padding-right: 10%;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.btns {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}