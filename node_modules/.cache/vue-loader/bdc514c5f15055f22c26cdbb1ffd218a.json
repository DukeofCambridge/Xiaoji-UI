gin: 18px 2%;width: 95%\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span><strong>课程介绍</strong></span>\r\n      </div>\r\n      <div class=\"text item\">\r\n        {{description}}\r\n      </div>\r\n    </el-card>\r\n  </el-row>\r\n  <el-row>\r\n    <el-card style=\"margin: 18px 2%;width: 95%\">\r\n      <div slot=\"header\" class=\"clearfix\">\r\n        <span><strong>历史通知</strong></span>\r\n        <el-button type=\"primary\" style=\"float: right\" >发布公告</el-button>\r\n      </div>\r\n      <el-table\r\n        :data=\"board\"\r\n        stripe\r\n        :default-sort = \"{prop: 'id', order: 'ascending'}\"\r\n        style=\"width: 100%\"\r\n        :max-height=\"tableHeight\">\r\n        <el-table-column\r\n          type=\"selection\"\r\n          width=\"55\">\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"title\"\r\n          label=\"通知标题\"\r\n          fit>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"date\"\r\n          label=\"发布时间\"\r\n          fit>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"120\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n              @click=\"editNotice(scope.row)\"\r\n              type=\"text\"\r\n              size=\"small\">\r\n              查看\r\n            </el-button>\r\n            <el-button\r\n              type=\"text\"\r\n              size=\"small\">\r\n              移除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <div style=\"margin: 20px 0 20px 0;float: left\">\r\n        <el-button>取消选择</el-button>\r\n        <el-button>批量删除</el-button>\r\n      </div>\r\n    </el-card>\r\n  </el-row>\r\n  <el-dialog\r\n    title=\"公告\"\r\n    :visible.sync=\"dialogFormVisible\">\r\n    <el-form v-model=\"selectedNotice\" style=\"text-align: left\" ref=\"dataForm\">\r\n      <el-form-item label=\"发布人\" label-width=\"120px\" prop=\"username\">\r\n        <label>{{selectedNotice.publisher}}</label>\r\n      </el-form-item>\r\n      <el-form-item label=\"详细内容\" label-width=\"120px\" prop=\"name\">\r\n        <label> {{selectedNotice.content}}</label>\r\n      </el-form-item>\r\n      <el-form-item label=\"日期\" label-width=\"120px\" prop=\"phone\">\r\n        <label> {{selectedNotice.date}}</label>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button type=\"warning\" @click=\"onSubmit(selectedNotice)\">编 辑</el-button>\r\n      <el-button type=\"primary\" @click=\"dialogFormVisible = false\">确 定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CourseHome',\r\n  data () {\r\n    return {\r\n      name: '软件工程',\r\n      courseId: '42034203',\r\n\r\n      description: '软件工程，你值得拥有',\r\n      // 公告板信息\r\n      board: [\r\n        {\r\n          id: 1,\r\n          title: '请同学们加入微信群balabala',\r\n          date: '2021.9.6',\r\n          content: '新的学期大家要元气满满哦~',\r\n          publisher: '黄杰'\r\n        },\r\n        {\r\n          id: 2,\r\n          title: '请同学们按时提交软件过程模型作业！',\r\n          date: '2021.10.6',\r\n          content: '截止时间10.10，内容为敏捷方法概述，各种过程模型的分析比较',\r\n          publisher: '黄杰'\r\n        }\r\n      ],\r\n\r\n      dialogFormVisible: true,\r\n      selectedNotice: [\r\n        {\r\n          id: 2,\r\n          title: '请同学们按时提交软件过程模型作业！',\r\n          date: '2021.10.6',\r\n          content: '截止时间10.10，内容为敏捷方法概述，各种过程模型的分析比较',\r\n          publisher: '黄杰'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    tableHeight () {\r\n      return window.innerHeight - 320\r\n    }\r\n  },\r\n  methods: {\r\n    editNotice (notice) {\r\n      this.dialogFormVisible = true\r\n      this.selectedNotice = notice\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n.clearfix:after {\r\n  clear: both\r\n}\r\n.text {\r\n  font-size: 14px;\r\n}\r\n\r\n.item {\r\n  margin-bottom: 18px;\r\n}\r\n</style>\r\n"]}]}