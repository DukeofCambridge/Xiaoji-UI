/el-table-column>\r\n          <el-table-column\r\n            prop=\"projectName\"\r\n            label=\"项目名称\"\r\n            fit>\r\n          </el-table-column>\r\n          <el-table-column\r\n            prop=\"projectTime\"\r\n            label=\"截止时间\"\r\n            sortable\r\n            fit>\r\n          </el-table-column>\r\n          <!--身份为教师时没有成绩列-->\r\n          <el-table-column\r\n            prop=\"score\"\r\n            label=\"成绩\"\r\n            fit>\r\n          </el-table-column>\r\n          <el-table-column\r\n            label=\"操作\"\r\n            width=\"120\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                @click=\"enter(scope.row)\"\r\n                type=\"text\">\r\n                查看\r\n              </el-button>\r\n              <!--身份为学生时没有评分功能-->\r\n              <el-button\r\n                type=\"text\">\r\n                评分\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        <div style=\"margin: 20px 0 20px 0;float: contour\">\r\n          <!--想到一个有意思的地方，可以根据学生总成绩动态更改分数的颜色-->\r\n          <span>总成绩:  {{23.55}}</span>\r\n        </div>\r\n      </el-card>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'CourseProjects',\r\n  data () {\r\n    return {\r\n      name: '软件工程',\r\n      courseId: '42034203',\r\n      // pastProjects: [\r\n      //   {\r\n      //     id: 12,\r\n      //     name: '项目一',\r\n      //     ddl: '2021.10.31',\r\n      //     score: 95.55,\r\n      //     status: 0\r\n      //   },\r\n      //   {\r\n      //     id: 1,\r\n      //     name: '考勤',\r\n      //     ddl: '',\r\n      //     score: 12.33,\r\n      //     status: 1\r\n      //   }\r\n      // ],\r\n      // currentProjects: [\r\n      //   {\r\n      //     id: 11,\r\n      //     name: '课程设计',\r\n      //     ddl: '2021.12.31',\r\n      //     status: 1\r\n      //   }\r\n      // ]\r\n      past: [],\r\n      now: []\r\n    }\r\n  },\r\n  computed: {\r\n    tableHeight () {\r\n      return window.innerHeight - 320\r\n    }\r\n  },\r\n  created() {\r\n    this.name = this.$store.state.coursename\r\n    this.courseId = this.$route.params.id\r\n    this.getPast()\r\n    this.getNow()\r\n  },\r\n  methods: {\r\n    getPast () {\r\n      let _this = this\r\n      console.log(this.$route.params.id)\r\n      this.$axios.get('/projects/'+this.$route.params.id+'/past').then(resp => {\r\n        if (resp && resp.data.code === 200) {\r\n          console.log('历史项目：', resp.data.object)\r\n          _this.past = resp.data.object\r\n        }\r\n      })\r\n    },\r\n    getNow(){\r\n      let _this = this\r\n      console.log(this.$route.params.id)\r\n      this.$axios.get('/projects/'+this.$route.params.id+'/now').then(resp => {\r\n        if (resp && resp.data.code === 200) {\r\n          console.log('当前项目：', resp.data.object)\r\n          _this.now = resp.data.object\r\n        }\r\n      })\r\n    },\r\n    enter(project){\r\n      let _this = this\r\n      _this.$router.push('/courses/'+_this.$route.params.id+'/project/'+project.projectId)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}